`timescale 1ns/10ps
module Multiplicador_tb();
	reg Clk, St, reset;
	reg [3:0]Multiplicando, Multiplicador;
	wire Idle, Done;
	wire [7:0]Produto; 
	integer i,j;
	Multiplicador DUT (.Clk(Clk), .St(St), .Done(Done), .Idle(Idle), 
					  .Multiplicando(Multiplicando), .Multiplicador(Multiplicador), 
					  .Produto(Produto), .reset(reset));
	
	initial begin
		Clk=0;reset=1;St=0;Multiplicando=1;Multiplicador=1;
		#10 reset=0;
		#10 St=1;
	end
	
	always begin
	#5 Clk=!Clk;
	end
	initial begin
	#20 for(i = 1; i<16; i = i+1)
		 begin
		 Multiplicando=1;
			for(j = 1; j<16; j = j+1)
			begin
				#100 Multiplicando = Multiplicando+1;
			end
			#100 Multiplicador = Multiplicador+1;
		 end
   $stop; 
	end
	
endmodule